{
  "name": "whiteapp",
  "version": "0.0.0-development",
  "main": "index.js",
  "author": "Foad Ardalan <tech-la@tech-la.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "start": "node ./dist/index",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .js,.ts",
    "coverage": "jest --coverage",
    "docker:build": "docker build . --build-arg NODE_VERSION=$(node --version | cut -d - -f 2) -t techla/whilteapp:lastest",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/techla/whiteapp.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "pinst": "^2.1.6",
    "semantic-release": "^17.4.4",
    "semantic-release-docker": "^2.2.0",
    "semantic-release-heroku": "^1.1.19",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "express": "^4.17.1",
    "fp-ts": "^2.10.5",
    "fp-ts-contrib": "^0.1.26",
    "hyper-ts": "^0.7.0-rc.1"
  }
